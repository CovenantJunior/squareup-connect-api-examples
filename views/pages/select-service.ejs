<html>

<head>
</head>

<body>
  <p> Select Service </p>
  <div class="services">
    <% items.forEach(function(item) { %>
      <% item.itemData.variations.forEach(function(variation) { %>
      <div class="dashboard__cards_card-wrapper">
        <a href="/services/<%= variation.id %>/staff">
          <%= item.itemData.name %>
          <%= item.itemData.description %>
          <%= variation.itemVariationData.name %>
          $<%= variation.itemVariationData.priceMoney.amount %>
        </a>
      </div>
      <%}); %>
    <%}); %>
</body>

</html>